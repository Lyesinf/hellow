   "source": [
    "print('Calculer le factoriel d\"un ombre')\n",
    "print('Donnez un nombre')\n",
    "x=input();x=int(x);i=1;y=x\n",
    "if x==0:\n",
    "    print('Le factoriel de 0 est 1')\n",
    "else:    \n",
    "    while i<y:\n",
    "        x=x*(y-i)\n",
    "        i=i+1\n",
    "    print('Le factoriel de '+str(y)+' est : '+str(x))    "
   ]
